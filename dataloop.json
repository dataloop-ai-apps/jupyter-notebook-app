{
    "version": "0.0.21",
    "name": "jupyter-notebook-onboarding-test",
    "displayName": "Jupyter Notebooks for Onboarding Test",
    "description": "This application provides a seamless way to learn how to use our SDK to perform various actions programmatically, which you can also accomplish through the Dataloop UI.",
    "scope": "project",
    "attributes": {
        "Hub": ["Dataloop"],
        "Provider": "Dataloop",
        "Category": "Application",
        "Application Type": "Notebook",
        "Media Type": ["All Media Types"]
    },
    "components": {
        "panels": [
            {
                "name": "models",
                "minRole": "annotator",
                "supportedSlots": [
                    {
                        "type": "floatingWindow",
                        "configuration": {
                            "layout": {
                                "width": 700,
                                "resizable": true,
                                "backgroundColor": "dl-color-studio-panel",
                                "onInvoke": true,
                                "mode": {
                                    "type": "fixed"
                                }
                            },
                            "routes": ["modelManagement"]
                        }
                    }
                ],
                "icon": "icon-dl-sdk-documentation",
                "metadata": {},
                "defaultSettings": {},
                "conditions": {}
            },
            {
                "name": "pipeline",
                "minRole": "annotator",
                "supportedSlots": [
                    {
                        "type": "floatingWindow",
                        "configuration": {
                            "layout": {
                                "width": 700,
                                "resizable": true,
                                "backgroundColor": "dl-color-studio-panel",
                                "onInvoke": true,
                                "mode": {
                                    "type": "fixed"
                                }
                            },
                            "routes": ["pipelines", "pipelinesEditor"]
                        }
                    }
                ],
                "icon": "icon-dl-sdk-documentation",
                "metadata": {},
                "defaultSettings": {},
                "conditions": {}
            },
            {
                "name": "data",
                "minRole": "annotator",
                "supportedSlots": [
                    {
                        "type": "floatingWindow",
                        "configuration": {
                            "layout": {
                                "width": 700,
                                "resizable": true,
                                "backgroundColor": "dl-color-studio-panel",
                                "onInvoke": true,
                                "mode": {
                                    "type": "fixed"
                                }
                            },
                            "routes": [
                                "datasetBrowser",
                                "dataManagement",
                                "project"
                            ]
                        }
                    }
                ],
                "icon": "icon-dl-sdk-documentation",
                "metadata": {},
                "defaultSettings": {},
                "conditions": {}
            }
        ],
        "modules": [
            {
                "name": "jupyter-notebook-module",
                "entryPoint": "functions/main.py",
                "className": "Runner",
                "initInputs": [],
                "config": {
                    "isCustomServer": true
                },
                "functions": [
                    {
                        "name": "run",
                        "output": [],
                        "input": []
                    }
                ]
            }
        ],
        "services": [
            {
                "name": "jupyter-notebook-serv",
                "panelNames": ["models", "pipeline", "data"],
                "moduleName": "jupyter-notebook-module",
                "initParams": {},
                "packageName": "default-package",
                "packageRevision": "latest",
                "runtime": {
                    "podType": "regular-s",
                    "concurrency": 10,
                    "runnerImage": "gcr.io/viewo-g/piper/agent/jupyter-server:2.0.8",
                    "autoscaler": {
                        "type": "rps",
                        "cooldownPeriod": 1200,
                        "threshold": 1,
                        "rateSeconds": 120,
                        "minReplicas": 0,
                        "maxReplicas": 1
                    },
                    "preemptible": false,
                    "executionTimeout": 3600,
                    "drainTime": 600,
                    "onReset": "failed",
                    "runExecutionAsProcess": false
                },
                "maxAttempts": 3
            }
        ]
    }
}
